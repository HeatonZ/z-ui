  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --host 0.0.0.0 --open",
    "build": "cross-env NODE_ENV=production webpack && cd dist && npx tsc -p ../tsconfig.json",
    "install": "yarn add -P react@17 react-dom@17 antd@4",
